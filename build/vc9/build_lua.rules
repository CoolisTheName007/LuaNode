<?xml version="1.0" encoding="utf-8"?>
<VisualStudioToolFile
	Name="Build Lua File"
	Version="8,00"
	>
	<Rules>
		<CustomBuildRule
			Name="Build Lua File"
			DisplayName="Build Lua File"
			CommandLine="..\..\tools\lua5.1.exe ..\..\tools\bin2c.lua $(InputPath) extension_status &gt; &quot;../../build/temp/$(ConfigurationName)/$(InputName).precomp&quot;"
			Outputs="../../build/temp/$(ConfigurationName)/$(InputName).precomp"
			FileExtensions="*.lua"
			>
			<Properties>
			</Properties>
		</CustomBuildRule>
	</Rules>
</VisualStudioToolFile>
